{
  "name": "lendpeak2",
  "version": "0.1.0",
  "description": "State-of-the-art loan management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:backend:health": "npm run dev & sleep 5 && npm run test:integration && kill $!",
    "test:unit": "jest tests/unit",
    "test:ui": "cd frontend && npm run test:run",
    "test:ui:watch": "cd frontend && npm run test",
    "test:ui:coverage": "cd frontend && npm run test:coverage",
    "test:functional": "jest tests/functional",
    "test:compliance": "jest tests/compliance",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "mongodb:start": "./scripts/start-mongodb.sh",
    "mongodb:stop": "./scripts/stop-mongodb.sh",
    "mongodb:status": "pgrep mongod > /dev/null && echo 'MongoDB is running' || echo 'MongoDB is not running'",
    "mongodb:logs": "tail -f data/logs/mongodb.log",
    "mongodb:console": "echo 'Note: mongosh not included with MongoDB 7.0. Install separately with: brew install mongosh'",
    "mongodb:reset": "npm run mongodb:stop && rm -rf data/db/* data/logs/* && npm run mongodb:start",
    "db:seed": "npm run db:seed-user && npm run seed:borrowers",
    "db:seed-user": "ts-node scripts/seed-user.ts",
    "seed:borrowers": "ts-node scripts/seed-borrowers.ts",
    "db:migrate": "ts-node scripts/run-migrations.ts",
    "db:backup": "ts-node scripts/backup-database.ts",
    "db:restore": "ts-node scripts/restore-database.ts",
    "services:start": "npm run mongodb:start && echo '✅ All services started'",
    "services:stop": "npm run mongodb:stop && echo '✅ All services stopped'",
    "services:status": "echo '=== Service Status ===' && echo '' && echo 'MongoDB:' && npm run mongodb:status",
    "services:logs": "npm run mongodb:logs",
    "start": "npm run services:start && npm run dev:all",
    "start:backend": "npm run services:start && npm run dev",
    "start:frontend": "npm run dev:frontend",
    "start:fresh": "npm run mongodb:reset && npm run db:seed && npm run dev:all",
    "stop": "npm run services:stop",
    "check": "./scripts/check-dependencies.sh",
    "postinstall": "cd frontend && npm install && cd .. && cd packages/lendpeak-engine && npm install && cd ../..",
    "cdk": "cdk"
  },
  "keywords": [
    "loan",
    "management",
    "financial",
    "lending"
  ],
  "author": "LendPeak2 Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
    "@aws-sdk/client-s3": "^3.817.0",
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@lendpeak/engine": "file:packages/lendpeak-engine",
    "@types/aws-sdk": "^0.0.42",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/swagger-ui-express": "^4.1.8",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "big.js": "^6.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.0",
    "multer": "^2.0.0",
    "react-dropzone": "^14.3.8",
    "socket.io": "^4.8.1",
    "stripe": "^12.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.0.0",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "aws-cdk": "^2.80.0",
    "aws-cdk-lib": "^2.80.0",
    "concurrently": "^9.1.2",
    "constructs": "^10.0.0",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
